{
  "name": "wysiwyg-ing",
  "version": "1.0.0",
  "description": "A beautiful, feature-rich WYSIWYG markdown editor for React with dual-mode editing, live preview, and extensive formatting options",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite demo",
    "build": "rollup -c",
    "build:demo": "vite build demo",
    "preview": "vite preview demo",
    "prepublishOnly": "npm run build",
    "test:build": "npm run build && npm run build:demo"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "wysiwyg",
    "markdown",
    "editor",
    "rich-text",
    "contenteditable",
    "formatting",
    "dark-mode",
    "split-view"
  ],
  "author": "Joseph Weitzel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BOXNYC/wysiwyg-ing.git"
  },
  "bugs": {
    "url": "https://github.com/BOXNYC/wysiwyg-ing/issues"
  },
  "homepage": "https://github.com/BOXNYC/wysiwyg-ing#readme"
}
